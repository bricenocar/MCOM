                                                },
                                                "EIMBlock": {
                                                    "type": "string"
                                                },
                                                "EIMBusinessArea": {
                                                    "type": "string"
                                                },
                                                "EIMCounterparty": {
                                                    "type": "string"
                                                },
                                                "EIMCountry": {
                                                    "type": "string"
                                                },
                                                "EIMDiscipline": {
                                                    "type": "string"
                                                },
                                                "EIMField": {
                                                    "type": "string"
                                                },
                                                "EIMInformationAsset": {
                                                    "type": "string"
                                                },
                                                "EIMLegalEntity": {
                                                    "type": "string"
                                                },
                                                "EIMLicense": {
                                                    "type": "string"
                                                },
                                                "EIMPlant": {
                                                    "type": "string"
                                                },
                                                "EIMProcess": {
                                                    "type": "string"
                                                },
                                                "EIMProcessArea": {
                                                    "type": "string"
                                                },
                                                "EIMSecurityClassification": {
                                                    "type": "string"
