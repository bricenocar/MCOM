                                                "EIMStatus": {
                                                    "type": "string"
                                                },
                                                "EIMWellbore": {
                                                    "type": "string"
                                                },
                                                "Edit": {
                                                    "type": "string"
                                                },
                                                "EditorLookupId": {
                                                    "type": "string"
                                                },
                                                "FileLeafRef": {
                                                    "type": "string"
                                                },
                                                "FilePath": {
                                                    "type": "string"
