                    "parameters": {
                        "$connections": {
                            "defaultValue": {},
                            "type": "Object"
                        }
                    },
                    "triggers": {
                        "Sliding_Window": {
                            "recurrence": {
                                "frequency": "Minute",
                                "interval": 10
                            },
                            "evaluatedRecurrence": {
                                "frequency": "Minute",
                                "interval": 10
                            },
                            "type": "SlidingWindow"
                        }
                    },
                    "actions": {
                        "For_each": {
                            "foreach": "@body('Get_rows_(V2)')?['value']",
                            "actions": {
                                "Parse_JSON": {
                                    "inputs": {
                                        "content": "@items('For_each')?['filemetadata']",
                                        "schema": {
                                            "properties": {
                                                "AuthorLookupId": {
                                                    "type": "string"
                                                },
                                                "BatchName": {
                                                    "type": "string"
                                                },
                                                "ContentType": {
                                                    "type": "string"
                                                },
                                                "Created": {
                                                    "type": "string"
                                                },
                                                "DocIcon": {
